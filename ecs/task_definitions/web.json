{
    "containerDefinitions": [
        {
            "command": [
                "./commands/web.sh"
            ],
            "cpu": 0,
            "environment": [
                {
                    "name": "POSTGRES_DB",
                    "value": "gptdb"
                },
                {
                    "name": "POSTGRES_USER",
                    "value": "postgres"
                },
                {
                    "name": "CELERY_QUEUE_NAME",
                    "value": "TD-queue"
                },
                {
                    "name": "DEBUG",
                    "value": "FALSE"
                },
                {
                    "name": "ALLOWED_HOSTS",
                    "value": "https://api.seasmoke.io"
                },
                {
                    "name": "CSRF_TRUSTED_ORIGINS",
                    "value": "api.seasmoke.io"
                },
                {
                    "name": "CORS_ALLOWED_ORIGINS",
                    "value": "http://localhost:3000,https://app.seasmoke.io,https://recruit.seasmoke.io"
                },
                {
                    "name": "CLOUDINARY_API_CLOUD_NAME",
                    "value": "dkhsao0yq"
                },
                {
                    "name": "CLOUDINARY_API_KEY",
                    "value": "979425196912885"
                }
            ],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://127.0.0.1/health-check || exit 1"
                ],
                "interval": 60,
                "retries": 3,
                "timeout": 59
            },
            "image": "112991276079.dkr.ecr.eu-west-2.amazonaws.com/td-api:2.0.33",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "TD-logs",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "mountPoints": [
                {
                    "containerPath": "/app/static",
                    "sourceVolume": "static_volume"
                }
            ],
            "name": "web",
            "portMappings": [],
            "secrets": [
                {
                    "name": "DJANGO_SECRET_KEY",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-2:112991276079:secret:TD-DJANGO-SECRET-KEY-QmVduB"
                },
                {
                    "name": "POSTGRES_HOSTNAME",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-2:112991276079:secret:TD-POSTGRES-HOSTNAME-jVgC8A"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-2:112991276079:secret:TD-POSTGRES-PASSWORD-Yei8Pq"
                },
                {
                    "name": "CELERY_ACCESS_KEY_ID",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-2:112991276079:secret:TD-CELERY_ACCESS_KEY_ID-wIbLqh"
                },
                {
                    "name": "CELERY_SECRET_ACCESS_KEY",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-2:112991276079:secret:TD-CELERY_SECRET_ACCESS_KEY-iX7Vbp"
                },
                {
                    "name": "OPENAI_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-2:112991276079:secret:OPENAI_API_KEY-hYCE1j"
                },
                {
                    "name": "PROXYCURL_TOKEN",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-2:112991276079:secret:PROXYCURL_TOKEN-eVTStW"
                },
                {
                    "name": "CLOUDINARY_API_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-2:112991276079:secret:CLOUDINARY_API_SECRET-hUgbjL"
                }
            ]
        },
        {
            "cpu": 256,
            "essential": true,
            "image": "public.ecr.aws/q7o3w5k9/custom-nginx:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "TD-logs",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "memory": 256,
            "name": "nginx",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "volumesFrom": [
                {
                    "sourceContainer": "web"
                }
            ]
        }
    ],
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::112991276079:role/TDTaskExecRole",
    "family": "td-api-web",
    "memory": "512",
    "networkMode": "awsvpc",
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "taskRoleArn": "arn:aws:iam::112991276079:role/TDTaskRole",
    "volumes": [
        {
            "host": {},
            "name": "static_volume"
        }
    ]
}
